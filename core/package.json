    "bumpp": "8.2.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "vite-plugin-dts": "1.4.1",
    "vite-plugin-libcss": "1.0.5",
    "vue-tsc": "0.34.17"
